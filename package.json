{
  "version": "1.3.11",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "dev": "tsdx watch",
    "prebuild": "rm -rf node_modules/tsdx/node_modules/typescript",
    "build": "tsdx build",
    "test": "clear ; ava",
    "test:watch": "clear ; ava --watch",
    "lint": "tsdx lint",
    "prepare": "tsdx build"
  },
  "prettier": {
    "printWidth": 100,
    "semi": true,
    "tabWidth": 4,
    "singleQuote": true,
    "parser": "typescript",
    "trailingComma": "es5"
  },
  "name": "wavematch",
  "author": "Chris Isler",
  "module": "dist/wavematch.esm.js",
  "devDependencies": {
    "@types/jest": "^24.0.25",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "ava": "^2.4.0",
    "eslint": "^8.51.0",
    "eslint-plugin-ava": "^9.0.0",
    "prettier": "^1.19.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.5.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@babel/generator": "^7.22.15",
    "@babel/parser": "^7.8.0",
    "@babel/types": "7.8.0"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "rules": {
      "no-case-declarations": "off"
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "root": true
  }
}
